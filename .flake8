[flake8]
max-line-length = 110
ignore =
    # Add any specific flake8 codes to ignore here if needed later
    # e.g., W503 line break before binary operator (black often does this)
    W503,
    # Ignore F401 for __init__.py files if needed:
    # F401
extend-exclude =
    # Add directories or files to exclude here if needed
    venv/
